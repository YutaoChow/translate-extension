{
    "manifest_version": 2,
    "name": "sf-translate",
    "description": "translate toll",
    "version": "1.0.0",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{ 
        "matches": [ "*://*.baidu.com/*" ],
        "css": [ "css/content.css" ],
        "js": [ "js/content.js" ],
        "run_at": "document_start" 
    }], 
    "web_accessible_resources": ["fonts/*"]
}